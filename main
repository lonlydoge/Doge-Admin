DogeAdmin.AddCommand("kill","Kills the included player",function(player)
	if DogeAdmin.GetShortenedPlrFromName(player) ~= nil then
        local Player
        for i, v in pairs(DogeAdmin.GetShortenedPlrFromName(player)) do
            Player = v
        pos = GetHRP().CFrame
	    local TargetCharacter = Player.Character
		if TargetCharacter:FindFirstChild("Time") then
        if TargetCharacter.Time.Value < GetCharacter().Time.Value then
        GetCharacter():ClearAllChildren()
        LocalPlayer.CharacterAdded:Wait()
        repeat wait() until FindFirstChild(GetCharacter(), "HumanoidRootPart")
        GetHRP().CFrame = pos
        elseif TargetCharacter.Time.Value == GetCharacter().Time.Value then
        GetCharacter():ClearAllChildren()
        LocalPlayer.CharacterAdded:Wait()
        repeat wait() until FindFirstChild(GetCharacter(), "HumanoidRootPart")
        GetHRP().CFrame = pos
        end
        end
        local Tool = LocalPlayer.Backpack:FindFirstChildOfClass("Tool")
        ReplaceHumanoid();
        pos = GetHRP().CFrame
        Equip(Tool)
        Tool.Handle.Size = Vector3.new(4,4,4)
        CFrameTool(Tool, TargetCharacter.HumanoidRootPart.CFrame)
       firetouchinterest(TargetCharacter.HumanoidRootPart, Tool.Handle, 0);
        firetouchinterest(TargetCharacter.HumanoidRootPart, Tool.Handle, 1);
        LocalPlayer.Character.Humanoid:ChangeState(15)
        wait(.3);
        LocalPlayer.Character:Destroy()
        LocalPlayer.CharacterAdded:Wait()
        repeat wait() until FindFirstChild(GetCharacter(), "HumanoidRootPart")
        GetHRP().CFrame = pos
	end
	else
	    DogeAdmin.Notify("Could not find player(s)", "Could not find player(s). perhaps username was/usernames were spelt wrong?", {5, 1, 1})
	end
end,"player")

