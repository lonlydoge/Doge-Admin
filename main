game:GetService("RunService").Heartbeat:Connect(function()
    LocalPlayer.MaximumSimulationRadius = math.pow(math.huge, math.huge) * math.huge
    pcall(function() sethiddenproperty(LocalPlayer, "SimulationRadius", math.pow(math.huge, math.huge) * math.huge) end)

    for i, v in pairs(game.Players:GetPlayers()) do
        if v ~= LocalPlayer then
            LocalPlayer.MaximumSimulationRadius = math.pow(math.huge, math.huge) * math.huge
            pcall(function() settings().Physics.AllowSleep = false ; sethiddenproperty(LocalPlayer, "SimulationRadius", math.pow(math.huge, math.huge) * math.huge) end)
            LocalPlayer.ReplicationFocus = Workspace
        end
    end
end)

function getChar()
    return LocalPlayer.Character
end


function replaceHum()
    if not getChar():FindFirstChild("HumanoidInjection") then
    local oldHum = getChar():FindFirstChildWhichIsA("Humanoid")
    oldHum.Name = "i"
    local newHum = oldHum:Clone()
    newHum.Parent = getChar()
    newHum.Name = "Humanoid"
    if not (syn) then
        wait();
    end
    oldHum:Destroy()
    workspace.CurrentCamera.CameraSubject = newHum
    spawn(function()
        local hI = Instance.new("StringValue",getChar())
        hI.Name = "HumanoidInjection"
    end)
    end
end
function checkIfToolHasHandle(tool)
    if tool:FindFirstChild("Handle") then
        return true
    else
        return false
    end
end
function getTool()
    local Tool = LocalPlayer.Backpack:FindFirstChildOfClass("Tool")
    if Tool then
        return Tool
    else
        return nil
    end
end
function isSat(Player)
    if Player.Character.Humanoid.Jump == true then
        return true
    else
        return false
    end
end

function tweenWSpeed(part,info,prop)
    local tween = ts:Create(part,info,prop)
    return tween
end

function getTimeAndReset(Player)
    pos = GetHRP().CFrame
	    local TargetCharacter = Player.Character
		if TargetCharacter:FindFirstChild("Time") then
        if TargetCharacter.Time.Value < GetCharacter().Time.Value then
        GetCharacter():ClearAllChildren()
        LocalPlayer.CharacterAdded:Wait()
        repeat wait() until FindFirstChild(GetCharacter(), "HumanoidRootPart")
        GetHRP().CFrame = pos
        elseif TargetCharacter.Time.Value == GetCharacter().Time.Value then
        GetCharacter():ClearAllChildren()
        LocalPlayer.CharacterAdded:Wait()
        repeat wait() until FindFirstChild(GetCharacter(), "HumanoidRootPart")
        GetHRP().CFrame = pos
        end
        end
end

function Sink(Player)
    spawn(function()
        if not isSat(Player) then
            getTimeAndReset(Player)
            local Tool = getTool();
            if checkIfToolHasHandle(Tool) then
                pos2 = GetHRP().CFrame
                replaceHum();
                Tool.Parent = getChar();
                Tool.Handle.Size = Vector3.new(4,4,4)
                CFrameTool(Tool, Player.Character.HumanoidRootPart.CFrame)
                if (not syn) then
                   wait(.1) 
                end
                for i2 = 1, 3 do
                firetouchinterest(Player.Character.HumanoidRootPart, Tool.Handle, 0);
                wait()
                firetouchinterest(Player.Character.HumanoidRootPart, Tool.Handle, 1);
                end
                repeat Heartbeat:wait() until Tool.Parent ~= getChar()
                tweent = true
                spawn(function()
                    while tweent do
                        Heartbeat:Wait()
                        getChar():FindFirstChildOfClass("Humanoid"):ChangeState(11)
                    end
                end)
                local tween = tweenWSpeed(getChar().HumanoidRootPart,TweenInfo.new(5),{CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,-10,0)});
                tween:Play()
                wait(3.5)
                tweent = false
                LocalPlayer.Character:Destroy()
                LocalPlayer.CharacterAdded:wait()
                repeat game:GetService("RunService").Heartbeat:wait() until LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                getHRP().CFrame = pos2
            end
        end
    end)
end

function Ascend(Player)
    spawn(function()
        if not isSat(Player) then
            getTimeAndReset(Player)
            local Tool = getTool();
            if checkIfToolHasHandle(Tool) then
                pos2 = GetHRP().CFrame
                replaceHum();
                Tool.Parent = getChar();
                Tool.Handle.Size = Vector3.new(4,4,4)
                CFrameTool(Tool, Player.Character.HumanoidRootPart.CFrame)
                if (not syn) then
                   wait(.1) 
                end
                for i2 = 1, 3 do
                firetouchinterest(Player.Character.HumanoidRootPart, Tool.Handle, 0);
                wait()
                firetouchinterest(Player.Character.HumanoidRootPart, Tool.Handle, 1);
                end
                tweent = true
                spawn(function()
                    while tweent do
                        Heartbeat:Wait()
                        getChar():FindFirstChildOfClass("Humanoid"):ChangeState(11)
                    end
                end)
                local tween = tweenWSpeed(getChar().HumanoidRootPart,TweenInfo.new(3),{CFrame = LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,320,0)});
                tween:Play()
                wait(3.3)
                tweent = false
                LocalPlayer.Character.Humanoid.Health = 0;
                LocalPlayer.Character = nil
                LocalPlayer.CharacterAdded:wait()
                repeat game:GetService("RunService").Heartbeat:wait() until LocalPlayer.Character:FindFirstChild("HumanoidRootPart")
                getHRP().CFrame = pos2
            end
        end
    end)
end


